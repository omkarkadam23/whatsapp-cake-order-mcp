{
    "cashfree": {
        "command": "node",
        "args": [
            "./cashfree-mcp/src/index.js"
        ],
        "env": {
            "PAYMENTS_APP_ID": "YOUR CF PAYMENTS APP ID",
            "PAYMENTS_APP_SECRET": "YOUR CF PAYMENTS APP SECRET",
            "ENV": "sandbox",
            "TOOLS": "pg"
        }
    },
    "whatsapp": {
        "command": "/usr/local/bin/uv",
        "args": [
            "--directory",
            "./whatsapp-mcp-server",
            "run",
            "main.py"
        ]
    }
}